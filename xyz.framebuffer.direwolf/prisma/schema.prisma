generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model wolfpack {
  id String @id @default(uuid())
  createdAt DateTime @default(now())
  timeTaken Float @default(0.0)
  sequenceNumber Int @default(autoincrement())
  resultCount Int 
  howlerName String
  testName String
  fileName String
  fileVersion String
  fileOrigin String
  wasCompleted Boolean @default(false)
  data String[]
}