generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Prey {
  id String @id @default(uuid())
  key String
  value String[]
  Wolfden Wolfden[]
}

model Wolfden {
  id String @id @default(uuid())
  queryName String
  createdAt DateTime @default(now())
  resultCount Int
  results Prey[]
}
